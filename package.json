{
  "name": "color-picker",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "ci": "npm run verify",
    "format": "prettier --write 'src/**/*.ts' tsconfig.json",
    "format-verify": "prettier --list-different 'src/**/*.ts' tsconfig.json",
    "start": "NODE_ENV=development webpack-dev-server --client-log-level warning --host 0.0.0.0",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run format-verify"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "author": "Denis Sokolov",
  "dependencies": {
    "@theorem/react": "^2.1.1",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.52",
    "@types/gapi.calendar": "^3.0.4",
    "@types/ms": "^0.7.31",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/seedrandom": "^2.4.28",
    "html-webpack-plugin": "^4.4.1",
    "ms": "^2.1.2",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "seedrandom": "^3.0.5",
    "ts-loader": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.0"
  }
}
